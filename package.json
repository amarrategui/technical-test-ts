{
  "name": "test-amdocs",
  "version": "1.0.0",
  "description": "",
  "author": {
    "email": "raul.valverde@amdocs.com",
    "name": "Raul Abril Valverde"
  },
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build:bundle": "npm run clean && npm run build && npm run bundle:prepare && npm run bundle:archive:app && npm run bundle:archive:layers",
    "build:bundle:docker": "npm run clean && npm run build && npm run bundle:prepare:docker && npm run bundle:archive:app && npm run bundle:archive:layers",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --maxWorkers=25%",
    "test:watch": "jest --watch --maxWorkers=25%",
    "test:cov": "jest --coverage --maxWorkers=25%",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "clean": "rimraf ./dist && rimraf ./dist-layers",
    "bundle:prepare": "mkdir -p dist-layers/nodejs && mkdir -p dist && cp package.json ./dist-layers/nodejs/ && cp package-lock.json ./dist-layers/nodejs/ && cd dist-layers/nodejs && npm install --only=prod && rimraf ./node_modules/aws-sdk",
    "bundle:prepare:docker": "mkdir -p dist-layers/nodejs && mkdir -p dist && cp package.json ./dist-layers/nodejs/ && cp package-lock.json ./dist-layers/nodejs/ && cd dist-layers/nodejs && npm install --only=prod",
    "bundle:archive:app": "cd dist/ && zip -qr build.zip .",
    "bundle:archive:layers": "cd dist-layers && zip -qr layers.zip ."
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.67.0",
    "@aws-sdk/client-lambda": "^3.215.0",
    "@aws-sdk/client-docdb": "^3.43.0",
    "@aws-sdk/client-sns": "^3.43.0",
    "@aws-sdk/client-sqs": "^3.43.0",
    "@aws-sdk/node-http-handler": "^3.43.0",
    "@nestjs/axios": "1.0.0",
    "@nestjs/common": "9.3.7",
    "@nestjs/config": "2.2.0",
    "@nestjs/core": "9.3.7",
    "@nestjs/event-emitter": "^1.0.0",
    "@nestjs/mongoose": "9.2.2",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "9.3.7",
    "@nestjs/serve-static": "3.0.0",
    "@nestjs/swagger": "6.2.1",
    "@types/axios": "^0.14.0",
    "@types/lru-cache": "^5.1.1",
    "@types/node-fetch": "^3.0.3",
    "@vendia/serverless-express": "^4.5.3",
    "agentkeepalive": "^4.1.4",
    "aws-lambda": "^1.0.7",
    "aws-serverless-express": "^3.4.0",
    "axios": "^0.23.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.14.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dbx-common": "file:node_packages/dbx-common-1.0.0.tgz",
    "dbx-tmf-docdb": "file:node_packages/dbx-tmf-docdb-4.1.0.tgz",
    "dbx-tmf-dto": "file:node_packages/dbx-tmf-dto-4.1.0.tgz",
    "dbx-tmf-joi": "file:node_packages/dbx-tmf-joi-4.1.0.tgz",
    "express": "^4.17.2",
    "fs": "0.0.1-security",
    "joi": "^17.5.0",
    "jsonpath": "^1.1.1",
    "jsonpath-plus": "^6.0.1",
    "jwk-to-pem": "^2.0.5",
    "jwks-rsa": "^3.0.0",
    "jwt-decode": "^3.1.2",
    "lru-cache": "^6.0.0",
    "mongodb": "^4.2.1",
    "mongoose": "6.11.5",
    "nest": "^0.1.6",
    "nest-winston": "^1.6.2",
    "passport": "^0.6.0",
    "passport-custom": "^1.1.1",
    "passport-jwt": "^4.0.0",
    "proxy-agent": "^5.0.0",
    "r2": "^2.0.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.4.0",
    "swagger-to-joi": "^1.2.6",
    "swagger-ui-express": "^4.2.0",
    "tunnel": "0.0.6",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
